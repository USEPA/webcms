{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "briancherne/jquery-hoverintent",
                "version": "v1.10.1",
                "type": "drupal-library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/briancherne/jquery-hoverIntent",
                    "reference": "v1.10.1"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "briancherne/jquery-hoverintent": "^1.10",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^1.27",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/back_to_top": "^1.0",
        "drupal/blizz_dynamic_entitylist": "^2.34",
        "drupal/ckeditor_abbreviation": "^1.5",
        "drupal/components": "^1.1",
        "drupal/config_ignore": "2.x-dev",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "~8.8.0",
        "drupal/core-recommended": "~8.8.0",
        "drupal/custom_add_another": "^1.0@beta",
        "drupal/default_content": "1.x-dev",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/elasticsearch_connector": "5.0-alpha3",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/field_group": "^1.0",
        "drupal/file_replace": "^1.1",
        "drupal/group": "1.x-dev",
        "drupal/groupmenu": "^1.0@beta",
        "drupal/layout_library": "^1.0@beta",
        "drupal/linkit": "5.x-dev",
        "drupal/linkit_media_creation": "^1.0@alpha",
        "drupal/masquerade": "^2.0@beta",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.9",
        "drupal/pathauto": "^1.4",
        "drupal/redis": "^1.1",
        "drupal/s3fs": "=3.0-alpha13",
        "drupal/search_api": "^1.14",
        "drupal/shs": "^1.0@alpha",
        "drupal/smtp": "^1.0@beta",
        "drupal/subpathauto": "^1.0@RC",
        "drupal/token": "^1.5",
        "drupal/twig_field_value": "^1.2",
        "drupal/twig_tweak": "^2.4",
        "drupal/views_autocomplete_filters": "^1.2",
        "drupal/webform": "^5.4",
        "drush/drush": "^9.0.0",
        "predis/predis": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "ext-gd": "1.0.0",
            "ext-opcache": "1.0.0",
            "ext-pdo": "1.0.0"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createFilesDir"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createFilesDir"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches-file": "composer.patches.json",
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}

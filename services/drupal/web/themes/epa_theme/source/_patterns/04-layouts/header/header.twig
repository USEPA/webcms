<div class="usa-overlay"></div>
<header class="l-header">
  <div class="l-constrain">
    <div class="l-header__navbar">
      <div class="l-header__branding">
        {% block logo %}
          <div style="background:#333;color:#fff;padding:1em;margin:0;">Logo</div>
        {% endblock %}
        <button class="usa-menu-btn">{{ 'Menu'|t }}</button>
      </div>
      <div class="l-header__search">
        {% block search %}
          <div style="background:#333;color:#fff;padding:1em;margin:0;">Search</div>
        {% endblock %}
      </div>
    </div>

  </div>
  <div class="l-header__nav">
    {% block navigation %}
      <div style="background:#333;color:#fff;padding:1em;margin:0;">Navigation</div>
    {% endblock %}
  </div>
</header>

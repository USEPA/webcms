{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/recommended",
        "@nuxtjs",
        "plugin:cypress/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "plugins": [
        "vue"
    ],
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".mjs",
                    ".js",
                    ".vue"
                ]
            }
        }
    },
    "rules": {
        "import/extensions": [
            "warning",
            "always",
            {
                "mjs": "never",
                "js": "always"
            }
        ],
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi-style": [
            1,
            "last"
        ],
        "lines-between-class-members": [
            1,
            "always"
        ],
        "function-paren-newline": [
            1,
            "never"
        ],
        "implicit-arrow-linebreak": [
            1,
            "beside"
        ],
        "for-direction": 2,
        "switch-colon-spacing": [
            1,
            {
                "after": true,
                "before": false
            }
        ],
        "array-bracket-newline": [
            1,
            {
                "multiline": true
            }
        ],
        "no-buffer-constructor": 2,
        "no-await-in-loop": 2,
        "prefer-promise-reject-errors": 2,
        "template-tag-spacing": [
            1,
            "never"
        ],
        "no-useless-return": 1,
        "func-name-matching": [
            1,
            "always"
        ],
        "class-methods-use-this": 1,
        "no-template-curly-in-string": 1,
        "no-return-await": 2,
        "require-await": 2,
        "symbol-description": 1,
        "no-compare-neg-zero": 2,
        "arrow-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "constructor-super": 2,
        "generator-star-spacing": [
            1,
            {
                "before": true,
                "after": false
            }
        ],
        "no-class-assign": 2,
        "no-const-assign": 1,
        "no-this-before-super": 2,
        "prefer-const": 0,
        "prefer-spread": 1,
        "require-yield": 2,
        "object-property-newline": [
            1,
            {
                "allowAllPropertiesOnSameLine": true
            }
        ],
        "object-shorthand": [
            1,
            "methods"
        ],
        "global-require": 1,
        "no-dupe-class-members": 2,
        "no-empty-pattern": 1,
        "no-new-symbol": 2,
        "no-self-assign": 1,
        "no-unused-labels": 1,
        "array-callback-return": 0,
        "no-empty-function": 1,
        "no-extra-label": 1,
        "import/no-extraneous-dependencies": 1,
        "no-useless-constructor": 1,
        "no-whitespace-before-property": 2,
        "prefer-rest-params": 1,
        "template-curly-spacing": 1,
        "yield-star-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "no-useless-escape": 1,
        "no-duplicate-imports": 2,
        "no-unsafe-finally": 2,
        "no-useless-computed-key": 1,
        "no-useless-rename": 1,
        "key-spacing": [
            1,
            {
                "afterColon": true
            }
        ],
        "new-cap": [
            1,
            {
                "newIsCap": true,
                "capIsNew": true
            }
        ],
        "new-parens": 1,
        "no-array-constructor": 1,
        "no-inline-comments": 1,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": [
            "error",
            "smart-tabs"
        ],
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "func-call-spacing": [
            1,
            "never"
        ],
        "no-trailing-spaces": 1,
        "no-unneeded-ternary": 1,
        "object-curly-spacing": [
            1,
            "never"
        ],
        "object-curly-newline": [
            1,
            {
                "minProperties": 3,
                "consistent": true
            }
        ],
        "operator-linebreak": [
            1,
            "after"
        ],
        "padded-blocks": [
            1,
            "never"
        ],
        "quote-props": [
            1,
            "as-needed"
        ],
        "quotes": [
            1,
            "single",
            "avoid-escape"
        ],
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "keyword-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "space-before-blocks": [
            1,
            "always"
        ],
        "space-before-function-paren": [
            1,
            "never"
        ],
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": 1,
        "space-unary-ops": [
            1,
            {
                "words": true
            }
        ],
        "spaced-comment": [
            1,
            "always",
            {}
        ],
        "max-len": [
            1,
            {
                "code": 125,
                "tabWidth": 2,
                "ignoreComments": false,
                "ignoreUrls": true,
                "ignoreTemplateLiterals": true,
                "ignoreStrings": true
            }
        ],
        "comma-dangle": [
            1,
            "always-multiline"
        ],
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "no-console": [
            "error",
            {
                "allow": [
                    "error"
                ]
            }
        ],
        "no-constant-condition": 1,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 1,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [
            1,
            "both"
        ],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-unsafe-negation": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 1,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-unexpected-multiline": 2,
        "block-scoped-var": 1,
        "consistent-return": 1,
        "curly": [
            1,
            "all"
        ],
        "dot-notation": 1,
        "dot-location": [
            1,
            "property"
        ],
        "eqeqeq": [
            1,
            "smart"
        ],
        "no-plusplus": 1,
        "no-caller": 2,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-floating-decimal": 1,
        "no-implied-eval": 2,
        "vue/attribute-hyphonation": 0,
        //"no-invalid-this": 2, false positives on browser event callbacks
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "no-global-assign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-param-reassign": 0,
        "no-prototype-builtins": 0,
        "no-proto": 2,
        "no-redeclare": [
            2,
            {
                "builtinGlobals": true
            }
        ],
        "no-return-assign": [
            2,
            "always"
        ],
        "no-script-url": 2,
        "no-self-compare": 1,
        "no-throw-literal": 2,
        "no-unused-expressions": 1,
        "no-underscore-dangle": "off",
        "no-useless-call": 1,
        "no-void": 1,
        "wrap-iife": [
            1,
            "inside"
        ],
        "yoda": [
            1,
            "never",
            {
                "exceptRange": true
            }
        ],
        "no-delete-var": 2,
        "no-label-var": 1,
        "no-shadow-restricted-names": 2,
        "no-shadow": [
            1,
            {
                "builtinGlobals": true,
                "hoist": "functions"
            }
        ],
        "default-case": 1,
        "no-continue": 1,
        "no-undef": 1,
        "no-undefined": 1,
        "no-unused-vars": 0,
        "no-use-before-define": [
            1,
            "nofunc"
        ],
        "callback-return": 2,
        "handle-callback-err": 1,
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "array-bracket-spacing": [
            1,
            "never",
            {}
        ],
        "brace-style": [
            1,
            "1tbs",
            {
                "allowSingleLine": true
            }
        ],
        "camelcase": [
            1,
            {
                "properties": "never"
            }
        ],
        "comma-spacing": [
            1,
            {
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "computed-property-spacing": [
            1,
            "never"
        ],
        "func-style": [
            1,
            "declaration",
            {
                "allowArrowFunctions": true
            }
        ],
        "semi": [
            "error",
            "always"
        ],
        "vue/max-attributess-per-line": "off",
        "vue/require-default-prop": "off",
        "vue/no-v-html": "off"
    }
}